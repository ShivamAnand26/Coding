#include<iostream>
using namespace std;

void InsertionSort(int *a, int n )
{
	for(int i=0; i<n; i++){
		int current = a[i];
		int j=i-1;
		
		while(a[j]>current && j>=0)
		{
			a[j+1] = a[j];
			j--;
		}
		a[j+1] = current;
	}
	cout<<"Sorted Data : ";
	for(int i=0; i<n; i++)
	{
		
		cout<<a[i]<<"->";
	}
}

int main(){
	int a[100];
	int n,i;
	
	cout<<"Enter the number of element in array : ";
	cin>>n;
	
	cout<<"Enter the elements : ";
	for(int i=0; i<n; i++)
	{
		cin>>a[i];
	}
	
	InsertionSort(a,n);
	
	return 0;
}
